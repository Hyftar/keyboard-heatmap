{
  "name": "keyboard-heatmap",
  "main": "./lib/keyboard-heatmap",
  "version": "0.3.0",
  "description": "An atom package to visualize how you have been using your keyboard",
  "keywords": [
    "keyboard",
    "usage",
    "statistics",
    "heatmap",
    "visual",
    "map",
    "heat"
  ],
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "repository": "https://github.com/hyftar/keyboard-heatmap",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
